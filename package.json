{
  "name": "nick-u2f-api",
  "description": "Promisified U2F API for browsers",
  "license": "MIT",
  "version": "0.0.1",
  "author": "Gustaf Räntilä <g.rantila@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/nitaliano/u2f-api.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "scripts/build.sh",
    "test": "node_modules/.bin/mocha dist/test",
    "buildtest": "npm run build && npm run test",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "cz": "git-cz"
  },
  "pre-commit": [
    "buildtest"
  ],
  "keywords": [
    "u2f",
    "api",
    "client",
    "2-factor",
    "authentication",
    "fido",
    "yubikey",
    "promise"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@types/mocha": "5.x",
    "already": "1.x",
    "chai": "4.x",
    "commitizen": "3.x",
    "cz-conventional-changelog": "2.x",
    "jsdom": "11.x",
    "mocha": "6.x",
    "pre-commit": "1.x",
    "rimraf": "2.x",
    "semantic-release": "15.x",
    "source-map-support": "0.x",
    "travis-deploy-once": "5.x",
    "typescript": "3.x"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
